                "${workspaceFolder}/staging/cpython/include/**",
                "${workspaceFolder}/staging/jsc/JavaScriptCore/Headers/JavaScriptCore",
                "${workspaceFolder}/staging/jsc/JavaScriptCore/PrivateHeaders/JavaScriptCore",
                "${workspaceFolder}/staging/jsc/WTF/Headers"

